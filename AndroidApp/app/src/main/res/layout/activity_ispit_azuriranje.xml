<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hmusovic.baze.IspitAzuriranjeActivity">

    <TextView
        android:id="@+id/i_textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="Kurs"
        android:textColor="#fff"
        android:textColorHighlight="#fff"
        android:textColorHint="#fff"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <Spinner
        android:id="@+id/i_planets_spinner_kurs"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:alpha="1"
        android:ems="10"
        android:fontFamily="sans-serif-condensed"
        android:inputType="textPersonName"
        android:textColor="#fff"
        android:textColorHighlight="#fff"
        android:textColorHint="#fff"
        android:textSize="20sp"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintTop_toBottomOf="@+id/i_textView6"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp" />

    <TextView
        android:id="@+id/i_textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="Sala"
        android:textColor="#fff"
        android:textColorHighlight="#fff"
        android:textColorHint="#fff"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/i_planets_spinner_kurs"
        app:layout_constraintHorizontal_bias="0.04"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <Spinner
        android:id="@+id/i_planets_spinner_sala"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:alpha="1"
        android:ems="10"
        android:fontFamily="sans-serif-condensed"
        android:inputType="textPersonName"
        android:textColor="#fff"
        android:textColorHighlight="#fff"
        android:textColorHint="#fff"
        android:textSize="20sp"
        android:layout_marginTop="7dp"
        app:layout_constraintTop_toBottomOf="@+id/i_textView8"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp" />

    <TextView
        android:id="@+id/i_textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="Tip ispita"
        android:textColor="#fff"
        android:textColorHighlight="#fff"
        android:textColorHint="#fff"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/i_planets_spinner_sala"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <Spinner
        android:id="@+id/i_planets_spinner_tipIspita"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:alpha="1"
        android:ems="10"
        android:fontFamily="sans-serif-condensed"
        android:inputType="textPersonName"
        android:textColor="#fff"
        android:textColorHighlight="#fff"
        android:textColorHint="#fff"
        android:textSize="20sp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/i_textView9"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp" />

    <EditText
        android:id="@+id/i_datumRok"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="8dp"
        android:clickable="true"
        android:hint="Unesite datum"
        android:textColor="#fff"
        android:textColorHighlight="#fff"
        android:textColorHint="#fff"
        android:inputType="none"
        android:editable="false"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/i_textView11" />

    <EditText
        android:id="@+id/i_vrijemeRok"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:inputType="none"
        android:editable="false"
        android:textColor="#fff"
        android:textColorHighlight="#fff"
        android:textColorHint="#fff"
        android:hint="Unesite vrijeme"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/i_textView11"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent" />


    <Button
        android:id="@+id/i_buttonAzurirajIspit"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:alpha="1"
        android:background="@drawable/button_round"
        android:fontFamily="sans-serif-condensed"
        android:text="Azuriraj"
        android:textColor="#fff"
        android:textColorHighlight="#fff"
        android:textColorHint="#fff"
        android:textSize="20sp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/i_datumIspit"
        android:layout_marginLeft="75dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/i_buttonObrisiIspit"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:alpha="1"
        android:background="@drawable/button_round"
        android:fontFamily="sans-serif-condensed"
        android:text="Obrisi"
        android:textColor="#fff"
        android:textColorHighlight="#fff"
        android:textColorHint="#fff"
        android:textSize="20sp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/i_vrijemeIspit"
        android:layout_marginRight="75dp"
        app:layout_constraintRight_toRightOf="parent" />
    <TextView
        android:id="@+id/i_textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="Rok za prijavu"
        android:textColor="#fff"
        android:textColorHighlight="#fff"
        android:textColorHint="#fff"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/i_planets_spinner_tipIspita"
        app:layout_constraintHorizontal_bias="0.04" />

    <TextView
        android:id="@+id/i_textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="Vrijeme ispita"
        android:textColor="#fff"
        android:textColorHighlight="#fff"
        android:textColorHint="#fff"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/i_datumRok"
        app:layout_constraintHorizontal_bias="0.04" />

    <EditText
        android:id="@+id/i_datumIspit"
        android:layout_width="0dp"
        android:layout_height="43dp"
        android:clickable="true"
        android:inputType="none"
        android:editable="false"
        android:hint="Unesite datum"
        android:layout_marginTop="8dp"
        android:textColor="#fff"
        android:textColorHighlight="#fff"
        android:textColorHint="#fff"
        app:layout_constraintTop_toBottomOf="@+id/i_textView12"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:id="@+id/i_vrijemeIspit"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:inputType="none"
        android:editable="false"
        android:textColor="#fff"
        android:textColorHighlight="#fff"
        android:textColorHint="#fff"
        android:hint="Unesite vrijeme"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/i_textView12"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent" />

</android.support.constraint.ConstraintLayout>

